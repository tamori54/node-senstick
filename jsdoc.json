{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "./lib",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "plugins": ["plugins/markdown"]
}